#!/usr/bin/env node

'use strict';

var seat = require('..');
var program = require('commander');

var pkg = require('../package');

program
  .option('-v, --versions', 'print put version infomation')
  .parse(process.argv);

if (program.versions) {
  console.info('\n  ' + pkg.version + '\n');
} else {
  seat();
}
